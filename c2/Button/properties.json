{
  "main": "{d.button}",
  "spacer": {
    "padding": {
      "_type": "var",
      "_replace": {
        "pattern": "^(.*?) (.*?)$",
        "replace": "$2 * 2"
      }
    },
    "_type": "var",
    "_settingClone": true
  },
  "gap": {
    "_type": "property",
    "_var": true
  },
  "padding": {
    "_type": "property"
  },
  "padding-spacer": {
    "width": "{??spacer.padding} + {??spacer.lineContainer.width} - ({??gap} * 2)"
  },
  "container": {
    "content": {
      "label": {
        "padding": {
          "_drag": "../../../../label",
          "_dragSetting": {
            "label": "{??label}"
          }
        },
        "_drag": "../../.."
      },
      "_drag": "../.."
    },
    "_type": "none",
    "_settingClone": true
  },
  "icon": {
    "left": {
      "_remove": true
    },
    "right": {
      "_remove": true
    },
    "only": {
      "_remove": true
    },
    "_type": "subclass",
    "_settingClone": true
  },
  "normal": {
    "_remove": true,
    "_settingClone": true
  },
  "skeleton": {
    "_remove": true,
    "_settingClone": true
  },
  "size": {
    "_default": "md"
  },
  "outline": {
    "background-color": {
      "_rename": "background-color"
    },
    "border-width": {
      "_type": "var"
    },
    "border-style": {
      "_type": "var"
    },
    "before": {
      "position": "absolute",
      "inset": "0px",
      "border-radius": "inherit",
      "border": "{??borderWidth} {??borderStyle}",
      "border-color": "inherit"
    }
  },
  "link": {
    "container": {
      "content": {
        "_remove": true
      },
      "label": {
        "_drag": "../.."
      },
      "_settingClone": true
    },
    "#label": {
      "border-bottom": "1px dashed"
    }
  },
  "intent": {
    "_settingClone": true
  },
  "primary": {
    "color": {
      "_rename": "color"
    }
  },
  "@@palette": "{palette}"
}
