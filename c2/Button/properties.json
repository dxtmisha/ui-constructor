{
  "main": "{d.button}",
  "padding": {
    "_type": "property"
  },
  "color": {
    "_rename": "palette-color"
  },
  "icon": {
    "left": {
      "_remove": true
    },
    "right": {
      "_remove": true
    },
    "only": {
      "_remove": true
    },
    "_type": "subclass",
    "_settingClone": true
  },
  "normal": {
    "_remove": true,
    "_settingClone": true
  },
  "skeleton": {
    "_remove": true,
    "_settingClone": true
  },
  "link": {
    "container": {
      "content": {
        "_remove": true
      },
      "label": {
        "_drag": "../.."
      },
      "_settingClone": true
    },
    "#label": {
      "border-bottom": "1px dashed"
    }
  },
  "intent": {
    "_settingClone": true
  },
  "primary": {
    "color": {
      "_rename": "color"
    },
    "background-color": {
      "_rename": "palette-background"
    }
  },
  "secondary": {
    "background-color": {
      "_rename": "palette-background"
    }
  },
  "outline": {
    "border-style": {
      "_type": "var"
    },
    "border-width": {
      "_type": "var"
    },
    "border-color": {
      "_rename": "palette-border"
    },
    "before": {
      "position": "absolute",
      "inset": "0px",
      "border-radius": "inherit",
      "border": "{??borderWidth} {??borderStyle}",
      "border-color": "inherit"
    }
  },
  "ghost": {

  },
  "@@palette": "{palette}"
}
