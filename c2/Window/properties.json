{
  "main": "{d.window}",
  "adaptive": {
    "~auto": {
      "autoBasic": "{??adaptive.modalDynamic}",
      "paddingTop": "{ot.dimension.300}",
      "paddingBottom": "{ot.dimension.300}",
      "media|sm": {
        "menuBasic": "{??adaptive.menu}"
      }
    },
    "~staticSm": {
      "media|sm": {
        "menuBasic": "{??adaptive.static}"
      }
    },
    "~staticMd": {
      "media|md": {
        "menuBasic": "{??adaptive.static}"
      }
    },
    "~staticLg": {
      "media|lg": {
        "menuBasic": "{??adaptive.static}"
      }
    },
    "_default": "auto"
  }
}
