{
  "name": "@dxtmisha/ui-constructor",
  "private": true,
  "version": "0.0.0-dev.3",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "ui": "vite-node scripts/comp.ts component",
    "build-ui": "vite-node scripts/comp.ts component && vue-tsc && vite build && vite-node scripts/comp.ts build"
  },
  "workspaces": [
    "packages/*"
  ],
  "files": [
    "book",
    "classes",
    "composables",
    "config",
    "constructors",
    "dist",
    "functions",
    "library",
    "media",
    "scripts",
    "styles",
    "types"
  ],
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/library/index.d.ts",
  "dependencies": {
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.35",
    "sass": "^1.71.0",
    "typescript": "^5.3.3",
    "vite-node": "^1.2.2",
    "vue": "^3.4.19",
    "vue-router": "^4.2.5",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-standard": "^8.0.1",
    "@vue/eslint-config-typescript": "^12.0.0",
    "commander": "^12.0.0",
    "dotenv": "^16.4.4",
    "eslint": "^8.56.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.21.1",
    "vite": "^5.1.0",
    "vite-plugin-css-injected-by-js": "^3.4.0",
    "vite-plugin-dts": "^3.7.2",
    "vue-tsc": "^1.8.27"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs",
      "types": "./dist/library/index.d.ts"
    },
    "./flags": {
      "import": "./dist/flags.js",
      "require": "./dist/flags.umd.cjs",
      "types": "./dist/library/flags.d.ts"
    },
    "./plugin": {
      "import": "./dist/plugin.js",
      "require": "./dist/plugin.umd.cjs",
      "types": "./dist/library/plugin.d.ts"
    },
    "./components": {
      "import": "./dist/components.js",
      "require": "./dist/components.umd.cjs",
      "types": "./dist/library/components.d.ts"
    },
    "./media": {
      "import": "./dist/media.js",
      "require": "./dist/media.umd.cjs",
      "types": "./dist/library/media.d.ts"
    },
    "./plugin-basic": {
      "import": "./dist/plugin-basic.js",
      "require": "./dist/plugin-basic.umd.cjs",
      "types": "./dist/library/plugin-basic.d.ts"
    },
    "./main": {
      "import": "./dist/main.js",
      "require": "./dist/main.umd.cjs",
      "types": "./dist/library/main.d.ts"
    },
    "./style.scss": "./library/style.scss",
    "./style-basic.scss": "./library/style-basic.scss",
    "./M3Image": {
      "import": "./dist/M3Image.js",
      "require": "./dist/M3Image.umd.cjs",
      "types": "./dist/m3/Image/index.d.ts"
    }
  }
}
